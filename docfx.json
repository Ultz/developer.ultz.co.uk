{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "ext/Silk.NET/src/**.csproj"
          ],
          "exclude": [
            "ext/Silk.NET/src/Lab/**.csproj",
            "ext/Silk.NET/src/Core/BuildTools/*.csproj"
          ]
        }
      ],
      "dest": "Silk.NET/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**.md",
          "**.yml"
        ],
        "exclude": [
          "ext/**.md",
          "ext/**.yml",
          "build/**.yml"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "resources/**",
          "**/resources/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "Silk.NET/apidoc/**.md"
        ],
        "exclude": [
        ]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "./template"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
